
+ (NSString*)version {
    return @"@OPENCV_VERSION@";
}

+ (NSString*)frameworkVersion {
    return @"opencv_darwin@OPENCV_VERSION_MAJOR@@OPENCV_VERSION_MINOR@@OPENCV_VERSION_PATCH@";
}

+ (NSInteger)versionMajor {
    return @OPENCV_VERSION_MAJOR@;
}

+ (NSInteger)versionMinor {
    return @OPENCV_VERSION_MINOR@;
}

+ (NSInteger)versionRevision {
    return @OPENCV_VERSION_PATCH@;
}

+ (NSString*)versionStatus {
    return @"@OPENCV_VERSION_STATUS@";
}
